function [oElec] = updElecMom(systemTot, alpha)
% update the nuclear momentum
global paramObj

coeff = MyConstants.ADAMS_FOURTH;
pre = paramObj.ringSize / paramObj.beta;

oElec = systemTot(1).electrons(alpha);

for time = 1:length(coeff)
    oElec.mom(:) = oElec.mom(:) + timeStep * coeff(time) / 24 * ...
        (-2 * pre * systemTot(time).electrons(alpha).pos(:) + ...
        pre / systemTot(time).theta * systemTot(time).electrons(alpha).dThdx(:));
end
end
