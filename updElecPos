function [oElec] = updElecPos(systemTot, alpha)
% update the nuclear momentum
global paramObj

coeff = MyConstants.ADAMS_FOURTH;
pre = paramObj.ringSize / paramObj.beta;

oElec = systemTot(1).electrons(alpha);

for time = 1:length(coeff)
    oElec.pos(:) = oElec.pos(:) + timeStep * coeff(time) / 24 * ...
        (2 * pre * systemTot(time).electrons(alpha).mom(:) - ...
        pre / systemTot(time).theta * systemTot(time).electrons(alpha).dThdp(:));
end
end
